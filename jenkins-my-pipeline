pipeline
{
    agent any
    
    environment{
        PATH = "/opt/apache-maven-3.8.1/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/ganjivasu/my-app'
            }
        }
        stage("Maven Build")
        {
            steps{
                sh "mvn clean package"
            }
        }
    }
}
